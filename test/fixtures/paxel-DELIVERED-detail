  <script>
    document.querySelectorAll( 'oembed[url]' ).forEach( element => {
        // const anchor = document.createElement( 'a' );
        // anchor.setAttribute( 'href', element.getAttribute( 'url' ) );
        // anchor.className = 'embedly-card';
        // element.appendChild( anchor );

        url = element.getAttribute( 'url' );

        if (url.includes("youtube")) {
          url = url.replace("watch?v=", "embed/").replace("&feature=youtu.be","");
        } else if(url.includes("youtu.be")) {
          url = url.replace("youtu.be","youtube.com/embed")
        }
        iframe = '<iframe class="embedded-iframe"src= "' + url + '" frameborder="0" allowfullscreen></iframe>';
        wrapperiframe = '<div class="embedded-videos">' + iframe + '</div>'
				element.parentElement.insertAdjacentHTML('afterend', wrapperiframe);

    } );

